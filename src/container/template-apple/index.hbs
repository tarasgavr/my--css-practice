<header>
  <section class="headerdark flex-justify-center">
    <div class="headerrow center1200">
      {{#with header}}
        <div class="headermenu">
          <img src="/img/svg/apple-sign.svg" alt="Apple" class="header-icon">
          {{#each links}}
            <a href="{{url}}" class="header-link">{{text}}</a>
          {{/each}}
        </div>
        <div class="header-signs">
          <img src="/img/svg/apple-search.svg" alt="Search" class="header-icon search-icon">
          <img src="/img/svg/apple-bag.svg" alt="Bag" class="header-icon">
        </div>
    </div>
  </section>
  <section class="headerlight flex-justify-center">
    <div class="headerrow center1200">
      {{#with ad}}
        <div class="header-title">{{title}}</div>
          {{#with link}}
            <a href="{{url}}" class="header-blue-link">{{text}}</a>
          {{/with}}
      {{/with}}
      {{/with}}
    </div>
  </section>
</header>
<main>
  <section class="iphone14pro flex-justify-center">
    <div class="product-info center1200">
      {{#with iphone_pro}}
        <h4 class="product-title">{{title}}</h4>
        <div class="product-description">{{descriptions}}</div>
        {{#with link}}
          {{#each text}}
            <a href="{{link}}" class="product-link">{{this}}</a>
          {{/each}}
        {{/with}}
      {{/with}}
    </div>
  </section>
</main>
<footer>

</footer>